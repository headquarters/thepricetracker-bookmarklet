{
  "name": "thepricetracker-bookmarklet",
  "version": "0.0.1",
  "description": "The bookmarklet for thepricetracker.com, used to save prices on websites. ",
  "main": "dist/bookmarklet.js",
  "scripts": {
    "start": "node index.js",
    "build": "bookmarklet ./dist/PriceTracker.js ./dist/PriceTrackerBookmarklet.js",
    "compile": "rollup -c rollup.config.js",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "test": "mocha --compilers js:babel-core/register --watch --recursive ./test"
  },
  "author": "",
  "license": "Unlicense",
  "devDependencies": {
    "bookmarklet": "0.0.6",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "prettier": "^1.7.4",
    "rollup": "^0.50.0",
    "rollup-plugin-svelte": "^3.1.0",
    "svelte-cli": "^1.4.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0"
  }
}
